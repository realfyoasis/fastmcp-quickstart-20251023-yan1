# Environment variables for local development
# DO NOT commit this file with real credentials

# Google Ads API
GOOGLE_ADS_DEVELOPER_TOKEN=your_developer_token_here
GOOGLE_ADS_CLIENT_ID=your_oauth_client_id_here
GOOGLE_ADS_CLIENT_SECRET=your_oauth_client_secret_here

# FastMCP OAuth Configuration (for multi-user auth)
FASTMCP_SERVER_AUTH=fastmcp.server.auth.providers.google.GoogleProvider
FASTMCP_SERVER_AUTH_GOOGLE_CLIENT_ID=your_oauth_client_id_here
FASTMCP_SERVER_AUTH_GOOGLE_CLIENT_SECRET=your_oauth_client_secret_here
FASTMCP_SERVER_AUTH_GOOGLE_BASE_URL=http://localhost:7070

# Optional: Manager account ID
# GOOGLE_ADS_LOGIN_CUSTOMER_ID=1234567890

# Server configuration
PORT=7070
HOST=0.0.0.0
